{
    "name": "laraflowai/laraflowai",
    "description": "A Laravel extension for building multi-agent AI workflows inspired by crewAI",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "LaraFlowAI Team",
            "email": "team@laraflowai.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/queue": "^10.0|^11.0",
        "illuminate/cache": "^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0",
        "symfony/expression-language": "^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "LaraFlowAI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaraFlowAI\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaraFlowAI\\LaraFlowAIServiceProvider"
            ],
            "aliases": {
                "FlowAI": "LaraFlowAI\\Facades\\FlowAI"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
